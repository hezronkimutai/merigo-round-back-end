{
  "name": "lostandfound",
  "version": "0.0.1",
  "description": "Lost and found",
  "main": "index.js",
  "scripts": {
    "test": "npm run undo-migration:test && npm run migrate:test && NODE_ENV=testing nyc --reporter=text --reporter=html --require @babel/polyfill --require @babel/register mocha ./src/v1/tests/*  --timeout 200000 --exit",
    "dev": "nodemon --exec babel-node src/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:test": "NODE_ENV=testing sequelize db:migrate",
    "undo-migration": "sequelize db:migrate:undo:all",
    "undo-migration:test": "NODE_ENV=testing sequelize db:migrate:undo:all",
    "build": "./node_modules/.bin/babel src -d dist && npm run buildNonJs",
    "start": "node dist/index.js",
    "buildNonJs": "babel-node ./src/v1/helpers/buildNonJsFiles buildNonJs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/william000000/LostAndFound.git"
  },
  "keywords": [
    "lost",
    "found",
    "lost and found"
  ],
  "author": "Willy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/william000000/LostAndFound/issues"
  },
  "homepage": "https://github.com/william000000/LostAndFound#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "babel-loader": "^8.0.6",
    "nodemon": "^2.0.2",
    "mocha": "^7.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  }
}
